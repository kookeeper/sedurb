<?xml version = '1.0' encoding = 'windows-1252'?>
<deployment-plan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.bea.com/ns/weblogic/deployment-plan http://www.bea.com/ns/weblogic/deployment-plan/1.0/deployment-plan.xsd"
                 xmlns="http://www.bea.com/ns/weblogic/deployment-plan">
   <application-name>RealizarAtendimentoUI</application-name>
   <variable-definition>
      <variable>
         <name>URL_WEBLOGIC</name>
         <value>t3://localhost:7001</value>
      </variable>
      <variable>
         <name>USUARIO_WEBLOGIC</name>
         <value>weblogic</value>
      </variable>
      <variable>
         <name>SENHA_WEBLOGIC</name>
         <value>welcome1</value>
      </variable>
   </variable-definition>
   <module-override>
      <module-name>SEDURBApp_RealizarAtendimentoUI_webapp1.war</module-name>
      <module-type>war</module-type>
      <module-descriptor external="false">
         <root-element>web-app</root-element>
         <uri>WEB-INF/web.xml</uri>
         <variable-assignment>
            <name>URL_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="URL_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
         <variable-assignment>
            <name>USUARIO_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="USUARIO_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
         <variable-assignment>
            <name>SENHA_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="SENHA_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
      </module-descriptor>
   </module-override>
   <module-override>
      <module-name>SEDURBApp_QualificarAtendimentoUI_webapp1.war</module-name>
      <module-type>war</module-type>
      <module-descriptor external="false">
         <root-element>web-app</root-element>
         <uri>WEB-INF/web.xml</uri>
         <variable-assignment>
            <name>URL_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="URL_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
         <variable-assignment>
            <name>USUARIO_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="USUARIO_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
         <variable-assignment>
            <name>SENHA_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="SENHA_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
      </module-descriptor>
   </module-override>
   <module-override>
      <module-name>SEDURBApp_ImprimirCarteiraUI_webapp1.war</module-name>
      <module-type>war</module-type>
      <module-descriptor external="false">
         <root-element>web-app</root-element>
         <uri>WEB-INF/web.xml</uri>
         <variable-assignment>
            <name>URL_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="URL_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
         <variable-assignment>
            <name>USUARIO_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="USUARIO_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
         <variable-assignment>
            <name>SENHA_WEBLOGIC</name>
            <xpath>/web-app/context-param/[param-name="SENHA_WEBLOGIC"]/param-value</xpath>
            <operation>replace</operation>
         </variable-assignment>
      </module-descriptor>
   </module-override>
</deployment-plan>
