                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam11}"
                                                         id="plam11">
                                  <af:inputDate label="Data Nascimento"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.idDataNascimento}"
                                                id="idDataNascimento"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Sexo:"
