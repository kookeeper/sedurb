                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Órgão Emissor RG:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam4}"
                                                         id="plam4">
                                  <af:inputText label="Órgão Emissor"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itIdentidadeOrgaoEmissor}"
