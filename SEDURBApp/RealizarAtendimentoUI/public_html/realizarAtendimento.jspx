<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <f:view>
    <af:document id="d1"
                 binding="#{backingBeanScope.backing_realizarAtendimento.d1}"
                 title="Realizar Atendimento">
      <af:messages binding="#{backingBeanScope.backing_realizarAtendimento.m1}"
                   id="m1"/>
      <af:form id="f1"
               binding="#{backingBeanScope.backing_realizarAtendimento.f1}">
        <af:pageTemplate viewId="/oracle/templates/threeColumnTemplate.jspx"
                         id="pt1"
                         partialTriggers="cb1 commandButton3 cbNovaConsulta cbConsultarCidadao">
          <f:facet name="center">
            <af:group binding="#{backingBeanScope.backing_realizarAtendimento.g1}"
