                                    <af:selectItem label="Feminino"
                                                   value="FEMININO"
                                                   binding="#{backingBeanScope.backing_realizarAtendimento.si2}"
                                                   id="si2"/>
                                  </af:selectOneRadio>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Telefone Residencial:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam9}"
                                                         id="plam9">
                                  <af:inputText label="Telefone Residencial"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itTelefoneResidencial}"
                                                id="itTelefoneResidencial"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Telefone Celular:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam8}"
                                                         id="plam8">
                                  <af:inputText label="Telefone Celular"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itTelefoneCelular}"
                                                id="itTelefoneCelular"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Número RG:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam7}"
                                                         id="plam7">
                                  <af:inputText label="Número" simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itIdentidadeNumero}"
                                                id="itIdentidadeNumero"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.carteiraIdentidade.numeroIdentidade}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Complemento RG:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam6}"
                                                         id="plam6">
                                  <af:inputText label="Complemento"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itIdentidadeComplemento}"
                                                id="itIdentidadeComplemento"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.carteiraIdentidade.numeroIdentidadeComplemento}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Data Emissão RG:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam5}"
                                                         id="plam5">
                                  <af:inputDate label="Data Emissão"
