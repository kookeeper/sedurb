                                       disclosed="true"
                                       binding="#{backingBeanScope.backing_realizarAtendimento.sdhDoc}"
                                       id="sdhDoc"
                                       rendered="#{not empty pageFlowScope.managedBean.cidadao}">
                    <af:table var="row" rowBandingInterval="0"
                              binding="#{backingBeanScope.backing_realizarAtendimento.tbDoc}"
                              id="tbDoc"
                              value="#{pageFlowScope.managedBean.listaDocumentoNecessarioAtendimento}"
                              styleClass="AFStretchWidth"
                              inlineStyle="height:149px;">
                      <af:column sortable="false" headerText="Nome Documento"
                                 align="start" id="c11" width="400">
                        <af:outputText value="#{row.nomeDocumento}" id="ot8"/>
                      </af:column>
                      <af:column sortable="false" headerText="Abrir Arquivo"
                                 align="center" id="c12">
                        <af:goLink text="Abrir Arquivo" id="gl1"
                                   destination="#{row.urlDocumento}"
                                   targetFrame="_blank"/>
                      </af:column>
                      <af:column sortable="false" headerText="Enviar Arquivo"
                                 align="center" id="column1">
                        <af:commandButton text="Certificar Documento"
                                          id="commandButton2"
                                          actionListener="#{pageFlowScope.managedBean.mostrarPopupUploadActionListener}"
                                          binding="#{backingBeanScope.backing_realizarAtendimento.commandButton2}">
                          <af:setPropertyListener from="#{row}"
                                                  to="#{pageFlowScope.managedBean.servicoAgendadoSelecionado}"
                                                  type="action"/>
                        </af:commandButton>
                      </af:column>
                    </af:table>
                  </af:showDetailHeader>
