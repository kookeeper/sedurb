                                      id="ph4">
                        <af:panelGridLayout binding="#{backingBeanScope.backing_realizarAtendimento.pgl7}"
                                            id="pgl7">
                          <af:gridRow marginTop="5px" marginBottom="5px"
                                      height="auto"
                                      binding="#{backingBeanScope.backing_realizarAtendimento.gr3}"
                                      id="gr3">
                            <af:gridCell marginStart="5px" width="auto"
                                         binding="#{backingBeanScope.backing_realizarAtendimento.gc3}"
                                         id="gc3">
                              <af:panelFormLayout binding="#{backingBeanScope.backing_realizarAtendimento.pfl3}"
                                                  id="pfl3" labelWidth="140">
                                <af:panelLabelAndMessage label="C.E.P.:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam13}"
                                                         id="plam13">
                                  <af:inputText label="C.E.P." simple="true" autoSubmit="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoCEP}"
                                                id="itEnderecoCEP" maximumLength="10"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.cep}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="País:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.panelLabelAndMessage1}"
                                                         id="panelLabelAndMessage1">
                                  <af:inputText label="País" simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoPais}"
                                                id="itEnderecoPais"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.pais}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Unidade Federativa:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.panelLabelAndMessage2}"
                                                         id="panelLabelAndMessage2">
                                  <af:selectOneChoice label="Unidade Federativa"
                                                      simple="true"
                                                      binding="#{backingBeanScope.backing_realizarAtendimento.socEnderecoUF}"
                                                      id="socEnderecoUF"
                                                      readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                      value="#{pageFlowScope.managedBean.cidadao.endereco.unidadeFederativa}">
                                    <f:selectItems value="#{pageFlowScope.managedBean.listaUnidadeFederativa}"
                                                   binding="#{backingBeanScope.backing_realizarAtendimento.si4}"
                                                   id="si4"/>
                                  </af:selectOneChoice>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Município:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.panelLabelAndMessage3}"
                                                         id="panelLabelAndMessage3">
                                  <af:inputText label="Município" simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoMunicipio}"
                                                id="itEnderecoMunicipio"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.municipio}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Bairro:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.panelLabelAndMessage4}"
                                                         id="panelLabelAndMessage4">
                                  <af:inputText label="Bairro" simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoBairro}"
                                                id="itEnderecoBairro"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.bairro}"/>
                                </af:panelLabelAndMessage>
                              </af:panelFormLayout>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="auto"
                                         binding="#{backingBeanScope.backing_realizarAtendimento.gc4}"
                                         id="gc4">
                              <af:commandButton text="Pesquisar C.E.P."
                                                binding="#{backingBeanScope.backing_realizarAtendimento.commandButton4}"
                                                id="commandButton4"
                                                rendered="#{pageFlowScope.managedBean.editandoCidadao}"
                                                actionListener="#{pageFlowScope.managedBean.pesquisarCEPActionListener}"
                                                partialSubmit="true"
                                                immediate="true"/>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" marginEnd="5px"
                                         width="auto"
                                         binding="#{backingBeanScope.backing_realizarAtendimento.gc5}"
                                         id="gc5">
                              <af:panelFormLayout binding="#{backingBeanScope.backing_realizarAtendimento.pfl4}"
                                                  id="pfl4" labelWidth="140">
                                <af:panelLabelAndMessage label="Complemento:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam14}"
                                                         id="plam14">
                                  <af:inputText label="Complemento"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoComplemento}"
                                                id="itEnderecoComplemento"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.complemento}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Número Logradouro:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.panelLabelAndMessage5}"
                                                         id="panelLabelAndMessage5">
                                  <af:inputText label="Número Logradouro"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoNumeroLogradouro}"
                                                id="itEnderecoNumeroLogradouro"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.numeroLogradouro}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Tipo Logradouro:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.panelLabelAndMessage6}"
                                                         id="panelLabelAndMessage6">
                                  <af:inputText label="Tipo Logradouro"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoTipoLogradouro}"
                                                id="itEnderecoTipoLogradouro"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.tipoLogradouro}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Nome Logradouro:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.panelLabelAndMessage7}"
                                                         id="panelLabelAndMessage7">
                                  <af:inputText label="Nome Logradouro"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itEnderecoNomeLogradouro}"
                                                id="itEnderecoNomeLogradouro"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.endereco.nomeLogradouro}"/>
                                </af:panelLabelAndMessage>
                                <f:facet name="footer">
                                  <af:panelGroupLayout binding="#{backingBeanScope.backing_realizarAtendimento.pgl1}"
                                                       id="pgl1">
                                    <af:commandButton text="Cancelar"
                                                      binding="#{backingBeanScope.backing_realizarAtendimento.cb2}"
                                                      id="cb2"
                                                      actionListener="#{pageFlowScope.managedBean.cancelarCadastroCidadaoActionListener}"
                                                      rendered="#{pageFlowScope.managedBean.editandoCidadao}">
                                      <af:resetActionListener/>
                                    </af:commandButton>
                                    <af:commandButton text="Concluir"
                                                      binding="#{backingBeanScope.backing_realizarAtendimento.cb3}"
                                                      id="cb3"
                                                      actionListener="#{pageFlowScope.managedBean.cadastrarCidadaoActionListener}"
                                                      rendered="#{pageFlowScope.managedBean.editandoCidadao}"/>
                                    <af:commandButton text="Atualizar Cadastro"
                                                      binding="#{backingBeanScope.backing_realizarAtendimento.commandButton1}"
                                                      id="commandButton1"
                                                      actionListener="#{pageFlowScope.managedBean.habilitarCadastroCidadaoActionListener}"
                                                      rendered="#{not pageFlowScope.managedBean.editandoCidadao}"/>
                                  </af:panelGroupLayout>
                                </f:facet>
                              </af:panelFormLayout>
                            </af:gridCell>
                          </af:gridRow>
                        </af:panelGridLayout>
                      </af:panelHeader>
                    </af:panelGroupLayout>
                  </af:showDetailHeader>
                </af:panelGroupLayout>
                <af:panelGroupLayout binding="#{backingBeanScope.backing_realizarAtendimento.pglDoc}"
