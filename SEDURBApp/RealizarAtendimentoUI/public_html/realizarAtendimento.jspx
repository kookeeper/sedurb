                                                         id="plam3">
                                  <af:inputText label="Nome Completo"
                                                simple="true"
                                                binding="#{backingBeanScope.backing_realizarAtendimento.itNomeCompleto}"
                                                id="itNomeCompleto"
                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.nomeCompleto}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Data Nascimento:"
