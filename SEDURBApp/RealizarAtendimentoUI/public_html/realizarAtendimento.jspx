                                                readOnly="#{not pageFlowScope.managedBean.editandoCidadao}"
                                                value="#{pageFlowScope.managedBean.cidadao.carteiraIdentidade.orgaoEmissor}"/>
                                </af:panelLabelAndMessage>
                                <af:panelLabelAndMessage label="Unidade Federativa RG:"
                                                         labelStyle="font-weight:bold; text-align:left;"
                                                         binding="#{backingBeanScope.backing_realizarAtendimento.plam12}"
                                                         id="plam12">
                                  <af:selectOneChoice label="Unidade Federativa"
                                                      simple="true"
                                                      binding="#{backingBeanScope.backing_realizarAtendimento.socIdentidadeUF}"
                                                      id="socIdentidadeUF"
                                                      readOnly="#{not pageFlowScope.managedBean.editandoCidadao}">
                                    <f:selectItems value="#{pageFlowScope.managedBean.listaUnidadeFederativa}"
                                                   binding="#{backingBeanScope.backing_realizarAtendimento.si3}"
                                                   id="si3"/>
                                  </af:selectOneChoice>
                                </af:panelLabelAndMessage>
                              </af:panelFormLayout>
                            </af:gridCell>
                          </af:gridRow>
                        </af:panelGridLayout>
                      </af:panelHeader>
                      <af:panelHeader text="Informações de Endereço do Cidadão"
